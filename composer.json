{
  "name": "elenyum/openapi",
  "version": "0.1",
  "type": "symfony-bundle",
  "description": "Generates OpenAPI documentation from entity",
  "keywords": ["api", "documentation", "doc", "rest"],
  "license": "MIT",
  "authors": [
    {
      "name": "Elenyum",
      "homepage": "https://elenyum.ru/bundles/openapi"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "symfony/console": "^5.4|^6.0|^7.0",
    "symfony/framework-bundle": "^5.4.24|^6.0|^7.0",
    "symfony/validator": "^7.0",
    "zircote/swagger-php": "^4.2.15"
  },
  "require-dev": {
    "doctrine/annotations": "^2.0",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpunit/phpunit": "^11.0",
    "symfony/options-resolver": "^7.0",
    "symfony/property-info": "^7.0",
    "symfony/serializer": "^7.0"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Elenyum\\OpenAPI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elenyum\\OpenAPI\\Tests\\": "tests/"
    }
  }
}
